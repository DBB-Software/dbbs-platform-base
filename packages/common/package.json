{
  "name": "@dbbs/common",
  "version": "0.0.0",
  "private": true,
  "description": "Common for dbbs project",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsc --build tsconfig.json",
