{
  "name": "@dbbs/server-api",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "type": "module",
  "scripts": {
    "build": "nest build",
    "clean": "npx rimraf .turbo node_modules dist bin *.tsbuildinfo",
    "dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:pm2": "node dist/cluster.js",
    "start:prod": "node dist/main",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --forceExit --coverage --verbose --testTimeout=10000000 --detectOpenHandles",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@dbbs/common": "*",
    "@dbbs/nestjs-module-authz": "*",
    "@dbbs/nestjs-module-health": "*",
    "@dbbs/nestjs-module-logger": "*",
    "@dbbs/s3-log-transport": "*",
    "@nestjs/common": "10.3.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/terminus": "10.2.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pino": "8.17.2",
    "pino-cloudwatch": "0.7.0",
    "pm2": "5.3.0"
  },
  "devDependencies": {
    "@dbbs/eslint-config": "*",
    "@dbbs/tsconfig": "*",
    "@dbbs/jest-config": "*"
  }
}
