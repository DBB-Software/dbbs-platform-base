{
  "name": "@dbbs/web-spa",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "npx rimraf .turbo node_modules dist bin coverage *.tsbuildinfo || exit 0",
    "build": "npx tsc && npx vite build",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "dev": "npx vite",
    "lint": "npx eslint . --ext ts,tsx --max-warnings 0",
    "preview": "npx vite preview",
    "test": "npx jest --coverage --verbose"
  },
  "dependencies": {
    "@dbbs/tailwind-components": "*",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@dbbs/cypress": "*",
    "@dbbs/eslint-config": "*",
    "@dbbs/jest-config": "*",
    "@dbbs/tsconfig": "*",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3"
  }
}
