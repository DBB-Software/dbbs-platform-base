{
  "name": "mobile-app",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "android": "npx react-native run-android",
    "clean": "npx rimraf .turbo node_modules dist bin coverage *.tsbuildinfo || exit 0",
    "ios": "npx react-native run-ios --scheme='MobileApp' --simulator='iPhone 15 Pro'",
    "lint": "npx eslint . --ext ts,tsx --max-warnings 0",
    "start": "npx react-native start",
    "test": "npx jest --coverage --verbose"
  },
  "dependencies": {
    "@dbbs/mobile-components": "*",
    "@react-native/metro-config": "0.73.5",
    "@react-navigation/bottom-tabs": "6.5.12",
    "@react-navigation/native": "6.1.10",
    "@react-navigation/stack": "6.3.21",
    "metro-config": "0.80.6",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "2.15.0",
    "react-native-monorepo-tools": "1.2.1",
    "react-native-paper": "5.12.3",
    "react-native-reanimated": "3.7.1",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "3.29.0",
    "react-native-vector-icons": "10.0.3"
  },
  "devDependencies": {
    "@dbbs/eslint-config": "*",
    "@dbbs/jest-config": "*",
    "@dbbs/tsconfig": "*"
  }
}
