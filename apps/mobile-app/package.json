{
  "name": "mobile-app",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "android": "npx react-native run-android",
    "clean": "npx rimraf .turbo node_modules dist bin coverage *.tsbuildinfo || exit 0",
