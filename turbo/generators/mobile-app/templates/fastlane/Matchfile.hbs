# Part of s3 integration for match, instead of using github or gitlab integration
storage_mode("s3")
s3_region(ENV["{{ appNameUpperCase }}_FASTLANE_S3_REGION"])
s3_bucket(ENV["{{ appNameUpperCase }}_FASTLANE_S3_BUCKET"])
s3_access_key(ENV["{{ appNameUpperCase }}_FASTLANE_S3_ACCESS_KEY"])
s3_secret_access_key(ENV["{{ appNameUpperCase }}_FASTLANE_S3_SECRET_ACCESS_KEY"])


team_id(ENV["{{ appNameUpperCase }}_TEAM_ID"])
team_name(ENV["{{ appNameUpperCase }}_TEAM_NAME"])
app_identifier([ENV["{{ appNameUpperCase }}_APPLE_APP_IDENTIFIER"]])

ENV['MATCH_PASSWORD'] = ENV["{{ appNameUpperCase }}_FASTLANE_MATCH_PASSWORD"]
