{
  "name": "@dbbs/{{ name }}",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "clean": "npx rimraf .turbo node_modules dist bin *.tsbuildinfo",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --forceExit --coverage --verbose --testTimeout=10000000 --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@dbbs/common": "*",
    "@dbbs/nestjs-module-health": "*",
    "@dbbs/nestjs-module-logger": "*",
    "@nestjs/common": "10.2.10",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.10",
    "@nestjs/terminus": "10.2.0",
    "pm2": "5.3.0"
  },
  "devDependencies": {
    "@dbbs/eslint-config": "*",
    "@dbbs/tsconfig": "*"
  }
}
