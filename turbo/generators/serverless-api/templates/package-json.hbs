{
  "name": "@dbbs/{{ name }}",
  "version": "2.0.0",
  "description": "Serverless API",
  "type": "module",
  "main": "./dist/lambda.js",
  "types": "./dist/lambda.d.ts",
  "scripts": {
    "build": "npx tsc --build tsconfig.json",
    "clean": "npx rimraf coverage .serverless .turbo node_modules dist *.tsbuildinfo",
    "dev": "npm run build && sls offline start --noTimeout --stage local",
    "lint": "npx eslint \"src/**/*.ts*\" --fix",
    "start": "nest start",
    "pretest": "npm run build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --forceExit --coverage --verbose --testTimeout=10000000 --detectOpenHandles",
    "watch": "npx tsc -w --preserveWatchOutput"
  },
  "dependencies": {
    "@dbbs/common": "*",
    "@dbbs/nestjs-module-health": "*",
    "@dbbs/nestjs-module-logger": "*",
    "@middy/core": "5.1.0",
    "@middy/do-not-wait-for-empty-event-loop": "5.1.0",
    "@middy/http-security-headers": "5.1.0",
    "@middy/input-output-logger": "5.1.0",
    "@nestjs/axios": "3.0.1",
    "@nestjs/common": "10.2.10",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.10",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "10.2.10",
    "@nestjs/terminus": "10.2.0",
    "@vendia/serverless-express": "4.10.4",
    "aws-lambda": "1.0.7",
    "express": "4.18.2",
    "form-data": "4.0.0",
    "helmet": "7.1.0",
    "jwks-rsa": "3.1.0",
    "ow": "1.1.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@dbbs/eslint-config": "*",
    "@dbbs/tsconfig": "*"
  }
}
