{
  "name": "@dbbs/{{ name }}",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "npx rimraf .turbo node_modules .next bin coverage *.tsbuildinfo || exit 0",
    "dev": "npx next dev",
    "build": "npx next build",
    "start": "npx next start",
    "lint": "npx next lint",
    "test": "npx jest --coverage --verbose",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "dev:sst": "npx sst bind next dev",
    "build:sst": "npx sst bind next build"
  },
  "dependencies": {
    {{#ifCond cssFramework '===' 'tailwind'}}
    "@dbbs/tailwind-components": "*",
    {{/ifCond}}
    {{#ifCond cssFramework '===' 'mui'}}
    "@dbbs/mui-components": "*",
    {{/ifCond}}
    "next": "14.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@dbbs/cypress": "*",
    "@dbbs/jest-config": "*"{{#ifCond cssFramework '===' 'tailwind'}},{{/ifCond}}
    {{#ifCond cssFramework '===' 'tailwind'}}
    "autoprefixer": "10.4.19",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.1"
    {{/ifCond}}
  }
}
