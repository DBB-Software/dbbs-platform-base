{
  "name": "@dbbs/platform",
  "workspaces": [
    "packages/log-transports/*",
    "packages/nestjs-modules/*",
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "dotenv -- turbo run build --filter ${npm_config_target:-\"*\"}",
    "clean:all": "npx turbo run clean && npm run clean:modules",
    "clean:modules": "npx rimraf node_modules",
    "cypress:open": "dotenv -- npx turbo run cypress:open --filter ${npm_config_target:-\"*\"}",
    "cypress:run": "dotenv -- npx turbo run cypress:run --filter ${npm_config_target:-\"*\"}",
    "dev": "dotenv -- npx turbo run dev --concurrency=99 --filter ${npm_config_target:-\"*\"}",
    "download:env": "bash ./download.env.sh $STAGE",
    "prelint": "npm run prettier",
    "lint": "npx turbo run lint --filter ${npm_config_target:-\"*\"}",
    "prettier": "npx prettier --write \"packages/**/*.{ts,tsx}\" \"apps/**/*.{ts,tsx}\"",
    "sort-packages": "npm i -g sort-package-json && sort-package-json && find packages/* apps/* -name package.json -maxdepth 2 -execdir npx sort-package-json \\; -execdir rm -rf node_modules \\;",
    "test": "dotenv -- npx turbo run test --filter ${npm_config_target:-\"*\"}",
    "update-packages": "npm i -g npm-check-updates && ncu -u && find packages/* apps/* -name package.json -maxdepth 2 -execdir npx ncu -u \\; -execdir rm -rf node_modules \\;"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.7",
    "@babel/runtime": "7.23.7",
    "@dbbs/eslint-config": "*",
    "@dbbs/tsconfig": "*",
    "@jest/globals": "29.7.0",
    "@nestjs/cli": "10.2.1",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.3.0",
    "@react-native/babel-preset": "0.74.0",
    "@react-native/eslint-config": "0.74.0",
    "@react-native/metro-config": "0.73.2",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/react-native": "12.4.3",
    "@tsconfig/node-lts": "20.1.1",
    "@turbo/gen": "1.11.3",
    "@types/aws-lambda": "8.10.130",
    "@types/capture-console": "1.0.5",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/lodash.isobject": "3.0.9",
    "@types/node": "20.10.6",
